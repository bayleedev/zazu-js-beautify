{
  "name": "zazu-js-beautify",
  "blocks": {
    "input": [
      {
        "id": "Beautify",
        "type": "Keyword",
        "keyword": "beautify",
        "title": "Beautify your clipboard's JavaScript!",
        "icon": "fa-code",
        "connections": [
          "Process"
        ]
      }
    ],
    "output": [
      {
        "id": "Process",
        "type": "UserScript",
        "script": "index.js",
        "connections": ["Copy", "Notify"]
      },
      {
        "id": "Copy",
        "type": "CopyToClipboard",
        "text": "{value}"
      },
      {
        "id": "Notify",
        "type": "SendNotification",
        "title": "Beautified!",
        "message": "Your beautiful JavaScript has been copied to your clipbaord."
      }
    ]
  }
}
